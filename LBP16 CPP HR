#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <iostream>
using namespace std;

int main() {
    string number;
    char digit;
    cin >> number;
    cin >> digit;
    int count = 0;
    
    for (char ch : number) {
        if (ch == digit) {
            count++;
        }
    }

  
    cout << count << endl;

    return 0;
}
